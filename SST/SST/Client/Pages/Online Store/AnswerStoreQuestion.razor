@page "/answerstorequestion/{TransactionID:guid}"
@layout EmptyLayout

<div class="content">
    <div class="card">
        <div class="card-body">
            <SST.Client.Components.TestComponent IsOnlineStore="true" TransactionID="TransactionID" />
        </div>
    </div>
</div>


@code {

    private Guid _transactionID;
    [Parameter]
    public Guid TransactionID
    {
        get
        {
            return _transactionID;
        }
        set
        {
            _transactionID = value;
        }
    }

}
