@page "/firmemailtemplate/{templateID:guid}"
@attribute [Microsoft.AspNetCore.Authorization.Authorize]
@namespace SST.Client

<div class="card">
    <div class="card-body">
        <h1>Maintain Email Template</h1>
        <h5>Upload Email Header</h5>
        <div class="drag-drop-zone">
            <InputFile OnChange="UploadHeader" MaxBufferSize="@MaxFileSize" />
            @headerStatus
        </div>
        <h5>Upload Email Footer</h5>
        <div class="drag-drop-zone">
            <InputFile OnChange="UploadFooter" MaxBufferSize="@MaxFileSize" />
            @footerStatus
        </div>
        <EditForm Model="template" OnValidSubmit="Update">
            <DataAnnotationsValidator />
            <ValidationSummary />

            <div class="form-group">
                <label for="name">Template Name</label>
                <InputText Id="name" Class="form-control" @bind-Value="template.Name" />
                <ValidationMessage For="@(() => template.Name)" />
            </div>

            <div class="form-group">
                <label for="subject">Subject</label>
                <InputText Id="subject" Class="form-control" @bind-Value="template.Subject" />
                <ValidationMessage For="@(() => template.Subject)" />
            </div>
            @if (headerFileName != null)
            {
                <h3>@headerFileName</h3>
                <img src="@template.HeaderBase64" />
            }
            <div class="form-group">
                <label for="body">Email Body</label>
                <InputTextArea Id="body" Class="form-control" @bind-Value="template.Body" />
                <ValidationMessage For="@(() => template.Body)" />
            </div>
            @if (footerFileName != null)
            {
                <h3>@footerFileName</h3>
                <img src="@template.FooterBase64" />
            }
            <button type="submit" class="btn btn-primary">Update</button>
        </EditForm>
    </div>
</div>


@code {
    [Parameter]
    public Guid TemplateID { get; set; }
    private EmailTemplateModel template = new EmailTemplateModel()
    {
        HeaderBase64 = "",
        FooterBase64 = "",
        Body = "Dear {FirstName} {LastName}," + Environment.NewLine + "Please follow the below link to reset your password." + Environment.NewLine + "{ResetLink}" + Environment.NewLine + "Regards" + Environment.NewLine + "SST Attorneys",
        Name = "Forgot Password",
        Subject = "Forgot Password"
    };

    const string DefaultStatus = "Drop a file here to view it, or click to choose a file";
    const int MaxFileSize = 5 * 1024 * 1024; // 5MB
    string headerStatus = DefaultStatus;
    string headerFileName;
    string headerFileTextContents;

    string footerStatus = DefaultStatus;
    string footerFileName;
    string footerFileTextContents;

    async Task UploadHeader(InputFileChangeEventArgs e)
    {
        var file = e.File;
        if (file == null)
        {
            return;
        }
        else if (file.Size > MaxFileSize)
        {
            headerStatus = $"That's too big. Max size: {MaxFileSize} bytes.";
        }
        else
        {
            headerStatus = "Loading...";

            byte[] buffer = new byte[0x1000];
            byte[] fileBytes = new byte[0];
            int numRead;
            using var stream = file.OpenReadStream(MaxFileSize);
            while ((numRead = await stream.ReadAsync(buffer, 0, buffer.Length)) != 0)
            {
                fileBytes = fileBytes.Concat(buffer).ToArray();
            }
            headerFileTextContents = Convert.ToBase64String(fileBytes);
            headerFileName = file.Name;

            headerStatus = DefaultStatus;
        }
    }

    async Task UploadFooter(InputFileChangeEventArgs e)
    {
        var file = e.File;
        if (file == null)
        {
            return;
        }
        else if (file.Size > MaxFileSize)
        {
            footerStatus = $"That's too big. Max size: {MaxFileSize} bytes.";
        }
        else
        {
            footerStatus = "Loading...";

            byte[] buffer = new byte[0x1000];
            byte[] fileBytes = new byte[0];
            int numRead;
            using var stream = file.OpenReadStream(MaxFileSize);
            while ((numRead = await stream.ReadAsync(buffer, 0, buffer.Length)) != 0)
            {
                fileBytes = fileBytes.Concat(buffer).ToArray();
            }
            footerFileTextContents = Convert.ToBase64String(fileBytes);
            footerFileName = file.Name;

            footerStatus = DefaultStatus;
        }
    }

    private async Task Update()
    {

    }
}
